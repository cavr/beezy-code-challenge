{
  "ci": {
    "collect": {
      "numberOfRuns": 3,
      "static-dist-dir": "./client/build",
      "settings": {
			  "chromeFlags": "--disable-gpu --no-sandbox --headless"
			}
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "offscreen-images": "off",
        "uses-webp-images": "off"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
